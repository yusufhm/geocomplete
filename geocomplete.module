/**
 *  Implements hook_ctools_plugin_directory().
 */
function geocomplete_ctools_plugin_directory($owner, $plugin_type) {
  if (in_array($owner, array('ctools', 'search_api_location'))) {
    return 'plugins/' . $plugin_type;
  }
}

